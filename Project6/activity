using System;
using System.ComponentModel;

class Activity
{
    public string Title { get; set; }
    public string Description { get; set; }
}
class Running
{
    public int Distance { get; set; }

  public Running(int distance)
}
{
    Distance = laps * 50 / 1000;
}
class Cycling
{
    public int Distance { get; set; }

  public Cycling(int speed)
  {
      Distance = laps * 50 / 1000 * 0.62;
  }
}
class Swimming
{
    public int Distance { get; set; }

  public Swimming(int laps)
  {
      Distance = laps;
  }
}
